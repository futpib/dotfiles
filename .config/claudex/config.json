{
  "packages": [
    "nodejs",
    "deno",
    "bun",
    "github-cli",
    "libyaml",
    "yarn",
    "python",
    "lynx"
  ],
  "volumes": [
    "~/.config/gh/",
    "~/.local/bin/"
  ],
  "env": {
    "PATH": "~/.rbenv/shims/:~/.local/bin/:~/.local/share/npm-global/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
  }
}
